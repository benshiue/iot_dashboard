extends layouts/default_sensor

block fb_meta
  meta(property='og:site_name', content='PartJS')
  meta(property='og:title', content='Next generation WoT and SPA.')
  meta(property='og:url', content='#{currentURL("")}')
  meta(property='og:image', content='')
  meta(property='article:published_time', content='')
  
block head
  title Next Generation WoT and SPA Framework

block neck

block feet
  script(src='/views/build/dashboard.js')

block body

  

  section
    .container(style='margin-top: 30px; margin-bottom: 80px;')
      section.spot-box
        .row.text-center
          .col-md-3.text-right
          .col-md-6#demo-spot

  section
    .container(style='width: 80%;')
      .or-spacer
        .mask
          |   
          span
            i Dashboard

  section
    .container(style='margin-top: 180px; margin-bottom: 80px;')
      section.spot-box
        .row.text-center#demo-accel
        .row.text-center#test-up


  script(type='text/template', id='tmpl-spot-news')
    .col-md-4
      h2 <%= lowpulseoccupancytime %>

  script(type='text/template', id='tmpl-accel')
    .col-md-6.col-md-offset-3
      .progress
        .progress-bar(role='progressbar', aria-valuenow='100', aria-valuemin='0', aria-valuemax='100', style='width: 100%;')
            span.sr-only 100%

  script(type='text/template', id='tmpl-spot-news')
    div
      .progress
        .progress-bar(role="progressbar", aria-valuenow="60", aria-valuemin="0", aria-valuemax="100", style!="width: <%= x %>%;")
      .progress
        .progress-bar(role="progressbar", aria-valuenow="60", aria-valuemin="0", aria-valuemax="100", style!="width: <%= y %>%;")
      .progress
        .progress-bar(role="progressbar", aria-valuenow="60", aria-valuemin="0", aria-valuemax="100", style!="width: <%= z %>%;")

  script(type='text/template', id='tmpl-test-up')
    h1 <%= ax %>, <%= ay %>, <%= az %>: 
